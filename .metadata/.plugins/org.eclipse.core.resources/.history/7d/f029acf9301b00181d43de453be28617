package novidadesJava8;

public class ExemploCursos {

	private String nome;
	private int alunos;
	
	public ExemploCursos(String nome, int alunos) {
		super();
		this.nome = nome;
		this.alunos = alunos;
	}
	public String getNome() {
		return nome;
	}
	public int getAlunos() {
		return alunos;
	}
	
	
}
